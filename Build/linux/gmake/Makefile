.NOTPARALLEL:

ifndef config
  config=debug_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x32)
  lua53_benchmark_config = debug_x32
  luajit_21_benchmark_config = debug_x32
endif
ifeq ($(config),debug_x64)
  lua53_benchmark_config = debug_x64
  luajit_21_benchmark_config = debug_x64
endif
ifeq ($(config),release_x32)
  lua53_benchmark_config = release_x32
  luajit_21_benchmark_config = release_x32
endif
ifeq ($(config),release_x64)
  lua53_benchmark_config = release_x64
  luajit_21_benchmark_config = release_x64
endif

PROJECTS := lua53_benchmark luajit_21_benchmark

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

lua53_benchmark:
ifneq (,$(lua53_benchmark_config))
	@echo "==== Building lua53_benchmark ($(lua53_benchmark_config)) ===="
	@${MAKE} --no-print-directory -C . -f lua53_benchmark.make config=$(lua53_benchmark_config)
endif

luajit_21_benchmark:
ifneq (,$(luajit_21_benchmark_config))
	@echo "==== Building luajit_21_benchmark ($(luajit_21_benchmark_config)) ===="
	@${MAKE} --no-print-directory -C . -f luajit_21_benchmark.make config=$(luajit_21_benchmark_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f lua53_benchmark.make clean
	@${MAKE} --no-print-directory -C . -f luajit_21_benchmark.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x32"
	@echo "  debug_x64"
	@echo "  release_x32"
